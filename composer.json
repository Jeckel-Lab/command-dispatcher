{
  "name": "jeckel-lab/command-dispatcher",
  "description": "Command dispatcher implementation",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Julien Mercier-Rojas",
      "email": "julien@jeckel-lab.fr"
    }
  ],
  "autoload": {
    "psr-4": {
      "JeckelLab\\CommandDispatcher\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\JeckelLab\\CommandDispatcher\\": "tests/"
    }
  },
  "require": {
    "php": ">=8.2",
    "psr/event-dispatcher": "*",
    "psr/container": "*",
    "jeckel-lab/contract": "~2.3",
    "psr/log": "*"
  },
  "require-dev": {
    "phpunit/phpunit": "^10",
    "phpmd/phpmd": "^2.14",
    "squizlabs/php_codesniffer": "^3.6",
    "phpro/grumphp": "^v2.0",
    "phpstan/phpstan": "^1.2",
    "roave/security-advisories": "dev-latest",
    "maglnet/composer-require-checker": "^4.7",
    "infection/infection": "^0.27",
    "pestphp/pest": "^v2.20"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true,
      "phpro/grumphp": true,
      "pestphp/pest-plugin": true
    }
  }
}
